{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": 0,
        "no-unused-vars": 0,
        "linebreak-style": 0,
        "comma-dangle": 1,
        "quotes": [2, "double"],
        "prettier/prettier": "error"
    },
    "plugins": ["prettier"]
}
