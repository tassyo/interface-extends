{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "max-len": "off",
    "no-unused-vars": "off",
    "linebreak-style": "off",
    "comma-dangle": "off",
    "prettier/prettier": "error",
    "no-underscore-dangle": "off",
    "import/prefer-default-export": "off",
    "import/extensions": "off"
  },
  "plugins": [
    "prettier"
  ]
}
